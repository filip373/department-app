<p id="notice"><%= notice %></p>

<h1>Listing Showrooms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Make</th>
      <th>Address</th>
      <th>Phone number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @showrooms.each do |showroom| %>
      <tr>
        <td><%= showroom.name %></td>
        <td><%= showroom.make %></td>
        <td><%= showroom.address %></td>
        <td><%= showroom.phone_number %></td>
        <td><%= link_to 'Show', showroom %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
